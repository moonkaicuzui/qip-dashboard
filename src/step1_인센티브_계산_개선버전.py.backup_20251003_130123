                    <tr><td>QA TEAM (QA3B)</td><td>ASSEMBLY INSPECTOR</td><td>400,000 VND</td></tr>
                    <tr><td>GROUP LEADER</td><td>GROUP LEADER</td><td>350,117 VND</td></tr>
                    <tr><td>LINE LEADER</td><td>LINE LEADER</td><td>194,668 VND</td></tr>
